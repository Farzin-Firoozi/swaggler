{
  "name": "swaggler",
  "version": "1.2.0",
  "description": "Swaggler helps you smuggle your existing API requests into structured, well-documented specs with ease",
  "main": "dist/index.js",
  "bin": {
    "swaggler": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "swaggler.png"
  ],
  "keywords": [
    "cli",
    "curl",
    "swagger",
    "openapi",
    "api",
    "documentation",
    "generator",
    "swagger-generator",
    "postman",
    "collection",
    "cli"
  ],
  "author": "Farzin Firoozi",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
